<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<stream>
	<name>test</name>
	<attributes>
		<attribute type="integer" name="temperature">
			<value type="min">0</value>
			<value type="max">25</value>
		</attribute>
		<attribute type="text" name="comment">
			<value type="min">1</value>
			<value type="max">10</value>
		</attribute>
		<attribute type="enumerate" name="sky">
			<value type="option">cloudy</value>
			<value type="option">sunny</value>
			<value type="option">rainy</value>
			<value type="option">stormy</value>
		</attribute>
		<attribute type="enumerate" name="pollution">
			<value type="option">low</value>
			<value type="option">medium</value>
			<value type="option">high</value>
		</attribute>
	</attributes>
</stream>